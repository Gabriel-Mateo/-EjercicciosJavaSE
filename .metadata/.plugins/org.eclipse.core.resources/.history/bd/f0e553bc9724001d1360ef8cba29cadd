package practicaMath;

public class UsoMath {

	public static void main(String[] args) {
		double numero = 100;
		double cuadrado = Math.sqrt(numero);
		double pI =  Math.PI;
		
		System.out.println("El cuadrado de " +numero+ " es: " +cuadrado);
		System.out.println(Math.sqrt(pI));
		
		int mumero = 100;
		
		System.out.println(Math.ceil(Math.random()));
		

	}

}
